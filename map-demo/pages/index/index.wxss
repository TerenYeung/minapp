/**index.wxss**/
@import '../../ui/tabs/tabs.wxss';
@import '/templates/car-window/car-window.wxss';
page {
  position: relative;

}

.index {
  width: 100vw;
  height: 100vh;
  overflow: hidden;
  color: #FFF;
  font-size: 28rpx;
}

.index-tabs {
  width: 100%;
  height: 80rpx;
  color: #FFF;
  background: #BC4F4A;

  position: absolute;
  top: 0;
  left: 0;
}

.index-tabs-item {
  margin-right: 50rpx;
}

map {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin-top: 80rpx;
}
